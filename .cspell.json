{
  "version": "0.2",
  "language": "en",
  "useGitignore": true,
  "ignorePaths": [
    ".cspell.json",
    ".devcontainer/",
    ".github/",
    ".vscode/",
    "docs/**/*.svg",
    "docs/**/*.xslt",
    "rust-toolchain.toml"
  ],
  "words": [
    "chrono",
    "clippy",
    "kibibytes",
    "rustlang",
    "rustup"
  ],
  "overrides": [
    {
      "filename": "**/*.md",
      "dictionaries": [
        "rust"
      ],
      "words": [
        "xsltproc"
      ]
    },
    {
      "filename": "**/*.rs",
      "words": [
        "doctest",
        "repr"
      ]
    }
  ]
}